{
  "name": "@sealsystems/semantic-release",
  "version": "4.0.20",
  "description": "Semantic Release executable and configuration for SEAL Systems",
  "dependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.3",
    "clone-deep": "4.0.1"
  },
  "devDependencies": {
    "@sealsystems/eslint-config-es": "3.6.2",
    "@sealsystems/mocha": "4.1.3"
  },
  "bin": {
    "release": "bin/release"
  },
  "scripts": {
    "bot": "bot",
    "licenses": "license-checker --production --csv",
    "lint": "lint",
    "lint:fix": "lint-fix",
    "release": "node bin/release",
    "test": "test-run",
    "test:coverage": "test-coverage",
    "test:coverage:check": "test-coverage-check",
    "test:report": "test-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sealsystems/node-semantic-release.git"
  },
  "author": {
    "name": "SEAL Systems AG",
    "email": "support@sealsystems.de",
    "url": "http://www.sealsystems.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sealsystems/node-semantic-release/issues"
  },
  "homepage": "https://github.com/sealsystems/node-semantic-release#readme",
  "release": {
    "extends": "@sealsystems/semantic-release/node-module"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
